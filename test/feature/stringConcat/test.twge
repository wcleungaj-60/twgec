__schema__ = "https://code.gamelet.com/gassets/schema/events/v1";
__title__ = "我的任務";
__map__ = "CG.TWEventsGameTemplate/gamemap.twmap";

def printWithPrefix(prefix, text) : actions {
    print(text = prefix + text);
}

block init {
    actions {
        printWithPrefix("prefix_", "text");
        print(text = "123" + "456" + "789");
    }
}