const pt = Point(8,8);

__schema__ = "https://code.gamelet.com/gassets/schema/events/v1";
__title__ = "我的任務";
__map__ = "CG.TWEventsGameTemplate/gamemap.twmap";
__skydowLocs__ = pt;

def testInt(v) : actions {
    actorTalk(duration = v);
}

def testBool(v) : actions {
    actorTalk(cleanTalk = v);
}

def testListPoint(v): actions {
    addActor(patrol = v);
}

def testPoint(v): actions {
    addActor(patrol = [v,Point(0,0),v]);
}

block init {
    actions {
        testInt(2 + 3 + 5 + 7);
        testInt(2 + 3 + 5 * 7);
        testInt(2 + 3 * 5 + 7);
        testInt(2 + 3 * 5 * 7);
        testInt(2 * 3 + 5 + 7);
        testInt(2 * 3 + 5 * 7);
        testInt(2 * 3 * 5 + 7);
        testInt(2 * 3 * 5 * 7);
        testInt(2 + 3 * (5 + 7));
        testInt((2 + 3) * 5 + 7);
        testInt((2 + 3) * (5 + 7));
        testBool(2 > 3 && 5 < 7);
        testBool(2 <= 3 || 5 >= 7);
        testBool("23" != "57");
        testBool("23" == "57");
        testListPoint([Point(1,1),Point(2,2)]);
        testPoint(Point(2,2));
        testListPoint([Point(1+1,1)]);
    }
}